      {
        "id": 5,
        "title": "Prediction Volume by Hour",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum by (hour) (increase(backtest_predictions_total[1h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
